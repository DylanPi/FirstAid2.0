/* pages/IDinfor/IDinfor.wxss */
.containe {
  padding: 0 60rpx;
}

.divLine {
  background: #E0E3DA;
  width: 100%;
  height: 5rpx;
  margin: 20rpx 0;
}

.info_img {
  height: 150rpx;
  width: 150rpx;
  border-radius: 200rpx;
}

.info_head {
  display: inline-block;
  width: 30%;
}

.info_text {
  display: inline-block;
  width: 70%;
}

.headblock {
  width: 100%;
}

.name {
  display: inline-block;
  font-size: 50rpx;
}

.ifreal {
  display: inline-block;
  width: 70%;
}

.list {
  flex: auto;
  height: 125rpx;
}

.list_cell {
  width: 25%;
  height: 100%;
  display: inline-block;
  text-align: center;
}

.num {
  margin: 15rpx auto;
}

.FAcard {
  width: 100%;
  height: 100rpx;
  padding-bottom: 20rpx;
}

.FAcard_img{
  width: 100%;
  height: 100rpx;
}
.act_cell{
  display: inline-block;
  width: 25%;
  height: 150rpx;
  text-align: center;
  margin-bottom: 80rpx;
}
.act_img{
  width: 70%;
  padding: 0 auto;
  height: 100%;
}

navigator {
height: 100%;
}